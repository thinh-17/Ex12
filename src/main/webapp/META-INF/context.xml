<?xml version="1.0" encoding="UTF-8"?>
<Context>
    <Resource name="jdbc/Project"
              auth="Container"
              type="javax.sql.DataSource"
              maxTotal="20"
              maxIdle="10"
              maxWaitMillis="10000"
              username="${DB_USER}"
              password="${DB_PASSWORD}"
              driverClassName="org.postgresql.Driver"
              url="${DB_URL}"/>
</Context>



